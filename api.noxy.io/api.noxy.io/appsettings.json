{
    "JWT": {

        "Secret": "[secret]",
        "Issuer": "https://api.noxyi.io",
        "Audience": "https://noxy.io",
        "Subject": "API Authorization"
    },
    "CORS": {
        "Origins": [ "http://localhost:3000" ],
        "Headers": "*",
        "Methods": "*"
    },
    "ConnectionStrings": {
        "MySQL": "server=[host];database=[db];user=[user];password=[password]"
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}
